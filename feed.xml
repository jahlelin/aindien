    <entry>
        <title>Guide To Digital Analytics Principles</title>
        <author>
            <name>Jason Moore</name>
        </author>
        <link href="https://aindien.com/guide-to-digital-analytics-principles.html"/>
        <id>https://aindien.com/guide-to-digital-analytics-principles.html</id>
